root = true

[*]
end_of_line = lf
insert_final_newline = true
indent_style = tab
indent_size = tab
tab_width = 2
charset = utf-8
trim_trailing_whitespace = true

[*.py]
indent_style = space
indent_size = 4
tab_width = 4

[*.ipynb]
indent_style = space
indent_size = 4
tab_width = 4

[package.json]
indent_style = space
